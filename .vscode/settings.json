{
  "files.associations": {
    "*.css": "scss"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cssVariables.lookupFiles": ["src/styles/variables.css"],
  "cSpell.words": ["Automerge", "localforage"]
}
